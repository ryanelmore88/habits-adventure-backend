# backend/.env - UPDATED with secure CORS
# Default Environment Configuration
ENVIRONMENT=development
NEPTUNE_ENDPOINT=localhost
NEPTUNE_PORT=8182
NEPTUNE_USE_SSL=false

# SECURE CORS - Explicit origins only (NO wildcards)
ALLOWED_ORIGINS=http://localhost:5173,http://127.0.0.1:5173,http://192.168.0.76:5173,http://localhost:3000

# Application Settings
APP_NAME=Habits Adventure Backend
DEBUG=true
LOG_LEVEL=INFO

# Debug Features (development only)
ENABLE_DEBUG_ENDPOINTS=true